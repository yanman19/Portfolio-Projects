            ercot: '',    // Add ERCOT link here
            miso: '',     // Add MISO link here
            pjm: '',      // Add PJM link here
            nyiso: '',    // Add NYISO link here
            nepool: ''    // Add NEPOOL link here
        };
        // ========================================

        // Add click handlers to each ISO region
        Object.keys(isoLinks).forEach(isoId => {
            const element = document.getElementById(isoId);
            if (element) {
                element.addEventListener('click', function() {
                    const link = isoLinks[isoId];
                    if (link && link.trim() !== '') {
                        window.open(link, '_blank');
                    } else {
                        console.log(`No link configured for ${isoId.toUpperCase()}`);
                    }
                });
            }
