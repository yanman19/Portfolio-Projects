import pandas as pd

# Sample dictionary of dataframes
dataframes_dict = {
    "df1": pd.DataFrame({"A": [1, 2, 3], "B": [4, 5, 6]}),
    "df2": pd.DataFrame({"A": [7, 8, 9], "B": [10, 11, 12]}),
}

# Adding a new column with the key name
for key, df in dataframes_dict.items():
    if isinstance(df, pd.DataFrame):
        df['KeyName'] = key
    else:
        print(f"Item with key '{key}' is not a DataFrame.")

# Filter only DataFrames
dataframes_list = [df for df in dataframes_dict.values() if isinstance(df, pd.DataFrame)]

# Concatenate all dataframes into one big dataframe
big_dataframe = pd.concat(dataframes_list, ignore_index=True)

# Display the big dataframe
print(big_dataframe)