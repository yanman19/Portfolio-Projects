import pandas as pd

# Example dictionary of dataframes
dataframes_dict = {
    'df1': pd.DataFrame({'DALoad': [19, 21, 23], 'oilGen': [1, 1, 1]}),
    'df2': pd.DataFrame({'DALoad': [18, 20.5, 22.5], 'oilGen': [1, 1, 1]})
}

# Add a new column with the dataframe name
for name, df in dataframes_dict.items():
    df['DataFrameName'] = name

# Concatenate all dataframes into a single dataframe
combined_df = pd.concat(dataframes_dict.values(), ignore_index=True)

# Display the combined dataframe
print(combined_df)